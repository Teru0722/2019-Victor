#include <kipr/botball.h>

int main()
{
    while(1 == 1){
    
    //find grey
    while(analog(0) > 1600){
    printf("grey\n");
    motor(0,100);
    motor(1,500);
    }
    
    //find white
    while(analog(0) < 1600){
    printf("white\n");
    motor(0,500);
    motor(1,100);
    }
    
    }

    return 0;
}

